package com.fresh.tree;

public class Node<T>
{
	Node<T> child;
	Node<T> parent;
	T data;
	
	public Node(Node<T> parent)
	{
		this.parent = parent;
	}
	
	public void setChild(Node<T> child)
	{
		this.child = child;
	}
	
	public Node<T> getChild()
	{
		return child;
	}
	
	public Node<T> getParent()
	{
		return parent;
	}
}
