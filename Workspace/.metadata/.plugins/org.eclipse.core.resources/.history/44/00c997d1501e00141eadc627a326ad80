package com.fresh.tree;

public class HuffmanTree<T>
{
	BinaryNode<T> root;
	public HuffmanTree(Byte[] bytes)
	{
		constructTree(bytes);
	}
	
	private void constructTree(Byte[] bytes)
	{
		for(int i = 0; i < bytes.length; i++)
		{
			int count = 0;
			for(int c = 0; c < bytes.length; c++)
			{
				if(bytes[i] == bytes[c])
				{
					count++;
				}
			}
			
		}
	}
}
