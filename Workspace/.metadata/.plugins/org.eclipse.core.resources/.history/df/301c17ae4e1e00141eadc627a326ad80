package com.fresh.tree;

public class BinaryNode<T>
{
	Node<T> left;
	Node<T> right;
	Node<T> parent;
	T data;

	public BinaryNode(T data, Node<T> parent)
	{
		this.parent = parent;
		this.data = data;
	}

	public void setChild(Node<T> child)
	{
		this.child = child;
	}

	public Node<T> getChild()
	{
		return child;
	}

	public Node<T> getParent()
	{
		return parent;
	}
}
